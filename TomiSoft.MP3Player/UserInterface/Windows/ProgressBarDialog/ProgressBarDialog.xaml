<Window x:Class="TomiSoft.MP3Player.UserInterface.Windows.ProgressBarDialog.ProgressBarDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TomiSoft.MP3Player.UserInterface.Windows.ProgressBarDialog"
        mc:Ignorable="d"
        Title="ProgressBarDialog" Height="113.91" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
		ShowInTaskbar="False">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="37*"/>
			<RowDefinition Height="15*"/>
			<RowDefinition Height="33*"/>
		</Grid.RowDefinitions>

		<Label Grid.Row="0" Content="Text here" Margin="5,0,5,0" x:Name="UI_Text" VerticalAlignment="Bottom"/>
		<ProgressBar Grid.Row="1" Margin="10,0,10,0" IsIndeterminate="True" x:Name="UI_ProgressBar"/>
	</Grid>
</Window>
